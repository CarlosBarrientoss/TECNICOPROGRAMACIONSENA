Algoritmo dividir_dos_numeros
	Definir num1 Como Real; definir num2 Como Real;
	
	Definir resultado Como Entero;
	Escribir "Ingresa el primer valor"; Leer num1;
	Escribir "Ingresa el segundo valor"; Leer num2;

	resultado<-num1/num2;
	;
	Escribir "el resultado de la división es: ",resultado;
	
	
FinAlgoritmo
