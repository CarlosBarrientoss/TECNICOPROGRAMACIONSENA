Proceso Division
	Definir Divisor Como Entero;
	Definir Dividendo Como Entero;
	Definir Resultado Como Entero;
	
	Escribir Sin Saltar "Ingresa el dividendo:";
	Leer Dividendo;
	Escribir Sin Saltar "Ingresa el divisor:";
	Leer Divisor;
	
	Resultado <- Dividendo/Divisor;
	Escribir "Su resultado es: ", Resultado;
	
FinProceso
