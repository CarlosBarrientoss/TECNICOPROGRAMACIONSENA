Algoritmo ejercicios_pseudocodigo
	Definir num como entero;
	Escribir "digite un numero del 1 al 5";
	Leer num;
	Si (num >  5) Entonces
		Escribir "el numero es mayor que 5";
	SiNo
		Si ( num = 2) Entonces
			Escribir "el numero 2 es primo";
		SiNo
			Si ( num = 3) Entonces
				Escribir "el numero 3 es primo";
			SiNo
				Si ( num = 5) Entonces
					Escribir "el numero 5 es primo";
				SiNo
					Escribir "el numero no es primo";
				Fin Si
			Fin Si
		Fin Si
	Fin Si
	
FinAlgoritmo
