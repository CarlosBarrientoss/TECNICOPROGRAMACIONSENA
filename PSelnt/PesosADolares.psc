Algoritmo PesosADolares
	Definir pesos,dolares,conversion como real;
	Escribir "Escribe la cantidad en Pesos";
	Leer pesos;
	
	dolar=4140;
	
	conversion=pesos/dolar;
	
	Escribir "la conversion de" ,pesos, " pesos convertido a dolares es: " ,conversion;
	
	Escribir "Muchas Gracias";
	
	
	
	
	
	
	
	
	
	
	
FinAlgoritmo
