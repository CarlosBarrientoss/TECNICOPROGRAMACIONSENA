Algoritmo CalculoObrero
	
	Definir salario, incremento, nuevo_salario Como Real;
	
	Escribir "";
	
	Escribir "Escribe tu salario: ";
	
	Leer salario;
	
	incremento = salario * .25
	nuevo_salario = salario +incremento
	
	
	Escribir "";
	
	Escribir "Tu nuevo salario es con un incremento del 25% es: ", nuevo_salario;
	
	
	
	
FinAlgoritmo
